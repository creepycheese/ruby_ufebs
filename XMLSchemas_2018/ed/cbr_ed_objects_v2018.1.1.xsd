<?xml version="1.0" encoding="Windows-1251"?>
<!--Центральный Банк Российской Федерации.
Унифицированные форматы электронных банковских сообщений. 

ППС
Схемы с описанием общих прикладных объектов электронных сообщений, используемых в расчетной сети Банка России.

-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ed="urn:cbr-ru:ed:v2.0" xmlns:lt="urn:cbr-ru:ed:leaftypes:v2.0" targetNamespace="urn:cbr-ru:ed:v2.0" elementFormDefault="qualified" version="2018.1.1">
	<xs:import namespace="urn:cbr-ru:ed:leaftypes:v2.0" schemaLocation="cbr_ed_leaftypes_v2018.1.1.xsd"/>
	<xs:annotation>
		<xs:documentation>Прикладные объекты электронных сообщений</xs:documentation>
	</xs:annotation>
	<xs:complexType name="AccDocRefID">
		<xs:annotation>
			<xs:documentation>Реквизиты исходного распоряжения.</xs:documentation>
			<xs:documentation>Settlement Document Attributes.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="AccDocNo" type="lt:AccountDocIDType" use="required">
			<xs:annotation>
				<xs:documentation>Номер распоряжения (поле 3).</xs:documentation>
				<xs:documentation>Initial Paper Document Number.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccDocDate" type="lt:DateType" use="required">
			<xs:annotation>
				<xs:documentation>Дата составления распоряжения (поле 4).</xs:documentation>
				<xs:documentation>Initial Paper Document Issue Date.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AccModifList">
		<xs:annotation>
			<xs:documentation>Список счетов</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Acc" type="lt:AccountNumberRUIDType" use="required">
			<xs:annotation>
				<xs:documentation>Лицевой счет</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccOperationType" type="lt:OneDigitCodeType" use="required">
			<xs:annotation>
				<xs:documentation>Признак операции со счетом</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AccBrfModifList">
		<xs:annotation>
			<xs:documentation>Список балансовых счетов</xs:documentation>
		</xs:annotation>
		<xs:attribute name="AccBrf" type="lt:AccountNumberRUIDBrfType" use="required">
			<xs:annotation>
				<xs:documentation>Балансовый счет</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccOperationType" type="lt:OneDigitCodeType" use="required">
			<xs:annotation>
				<xs:documentation>Признак операции со счетом</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AccIntradayRestrictions">
		<xs:annotation>
			<xs:documentation>Ограничения по счету.</xs:documentation>
			<xs:documentation>Intraday restrictions on an account.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="StopDebits" type="ed:IntradayRestrictionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Остановка списания.</xs:documentation>
					<xs:documentation>Stop debits.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StopAllDebits" type="ed:IntradayRestrictionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Остановка всех списаний.</xs:documentation>
					<xs:documentation>Stop all debits.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StopCredits" type="ed:IntradayRestrictionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Остановка зачисления.</xs:documentation>
					<xs:documentation>Stop credits.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StopUrgent" type="ed:IntradayRestrictionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Остановка предоставления сервиса срочного перевода денежных средств.</xs:documentation>
					<xs:documentation>Stop providing urgent fund transfer service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PendingDeletion" type="ed:IntradayRestrictionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>В ожидании удаления.</xs:documentation>
					<xs:documentation>Pending deletion.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteToCBR" type="ed:IntradayRestrictionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Перенаправление зачислений в обслуживающее подразделение Банка России.</xs:documentation>
					<xs:documentation>Route credits to serving CBR subdivision.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ManualVerifMode" type="ed:IntradayRestrictionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Предварительный контроль ПБР.</xs:documentation>
					<xs:documentation>Manual verification mode.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Arrest" type="ed:IntradayRestrictionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Арест или ограничение на неопределенную сумму.</xs:documentation>
					<xs:documentation>Arrest or limitation for indeterminate amount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SpecialMode" type="ed:IntradayRestrictionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Наличие к счету очереди неисполненных в срок распоряжений.</xs:documentation>
					<xs:documentation>The system shall support a “special mode” attribute, which shall apply at the account level.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AccMigrated">
		<xs:annotation>
			<xs:documentation>Информация, связанная с процессом миграции счетов Участника.</xs:documentation>
			<xs:documentation>Information regarding Participant’s Migration from RABIS.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="BalanceReceived" type="lt:IndicatorType" use="required">
			<xs:annotation>
				<xs:documentation>Данные об остатке на счете получены в АС ППС.</xs:documentation>
				<xs:documentation>Balance was received in NPS from RABIS.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AccOI">
		<xs:annotation>
			<xs:documentation>Информация по ответисполнителям</xs:documentation>
		</xs:annotation>
		<xs:attribute name="BICOI" type="lt:BICRUIDType" use="required">
			<xs:annotation>
				<xs:documentation>БИК подразделения ответисполнителя</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OINo" type="lt:UserIDType" use="required">
			<xs:annotation>
				<xs:documentation>Номер ответисполнителя, которому даны права к счету</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OISurname" type="lt:Max50TextType" use="required">
			<xs:annotation>
				<xs:documentation>Фамилия ответисполнителя</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OIName" type="lt:Max50TextType" use="required">
			<xs:annotation>
				<xs:documentation>Имя ответисполнителя</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OIPatronymic" type="lt:Max50TextType">
			<xs:annotation>
				<xs:documentation>Отчество ответисполнителя(при наличии)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccessType" type="lt:OneDigitCodeType" use="required">
			<xs:annotation>
				<xs:documentation>Тип доступа к счету</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AccountsInfoType">
		<xs:annotation>
			<xs:documentation>Информация о счетах, входящая в профиль участника.</xs:documentation>
			<xs:documentation>Participant’s Account Information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AccIntradayRestrictions" type="ed:AccIntradayRestrictions" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ограничения по счету.</xs:documentation>
					<xs:documentation>Intraday restrictions on an account.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PendingDeletionInfo" type="ed:PendingDeletionInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дополнительная информация для ограничения «в ожидании удаления» (информация о предстоящем удалении счета).</xs:documentation>
					<xs:documentation>Additional information for “Pending deletion” restriction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceSweepingAccount" type="ed:BalanceSweepingAccount" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Счет назначения.</xs:documentation>
					<xs:documentation>Balance Sweeping Destination Account.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PoolSPInfo" type="ed:PoolSPInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация об участниках пула ликвидности.</xs:documentation>
					<xs:documentation>Primary account of liquidity pool (LP) OR List of Subordinate Accounts of LP.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IPInfo" type="ed:IPListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация о косвенных участниках: Список БИК косвенных участников для прямого.</xs:documentation>
					<xs:documentation>Indirect Participant Info: List of Indirect Participant BIC’s for Direct Participant.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LimitsInfo" type="ed:LimitsInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Значения лимитов.</xs:documentation>
					<xs:documentation>Participant Limits Information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AuthorizedClaimAuthors" type="ed:AuthorizedClaimAuthors" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Список уполномоченных составителей требований.</xs:documentation>
					<xs:documentation>Authorized Claim Authors List.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArrestsInfo" type="ed:ArrestsInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация об арестах и/или ограничениях.</xs:documentation>
					<xs:documentation>Arrests/Limitations Info.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AccMigrated" type="ed:AccMigrated" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация, связанная с процессом миграции счетов Участника.</xs:documentation>
					<xs:documentation>Information regarding Participant’s Migration from RABIS.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Accnt" type="lt:AccountNumberRUIDType" use="required">
			<xs:annotation>
				<xs:documentation>Номер счета.</xs:documentation>
				<xs:documentation>Account Number.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccountType" type="lt:AccountType" use="required">
			<xs:annotation>
				<xs:documentation>Типа номера счета.</xs:documentation>
				<xs:documentation>Account Type.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DateOfDeletion" type="lt:DateType">
			<xs:annotation>
				<xs:documentation>Дата фактического исключения информации о счете.</xs:documentation>
				<xs:documentation>Scheduled date of account’s deletion.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DefaultARMNo" type="lt:Max2TextType">
			<xs:annotation>
				<xs:documentation>Номер АРМ по умолчанию для счета.</xs:documentation>
				<xs:documentation>Default Terminal ID for Account.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AccountsType">
		<xs:annotation>
			<xs:documentation>Информация о счетах участника перевода денежных средств.</xs:documentation>
			<xs:documentation>Participant’s Accounts information.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Accnt" type="lt:AccountNumberRUIDType" use="required">
			<xs:annotation>
				<xs:documentation>Номер счета.</xs:documentation>
				<xs:documentation>Account Number.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RegulationAccountType" type="lt:AccountType" use="required">
			<xs:annotation>
				<xs:documentation>Типа счета.</xs:documentation>
				<xs:documentation>Account Type.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CK" type="lt:Max2TextType">
			<xs:annotation>
				<xs:documentation>Контрольный ключ.</xs:documentation>
				<xs:documentation>Control Key.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccntCBRBIC" type="lt:BICRUIDType" use="required">
			<xs:annotation>
				<xs:documentation>БИК ПБР, обслуживающего счет участника перевода.</xs:documentation>
				<xs:documentation>CBR Subdivision serving the account.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccRstr" type="lt:Max1TextType" use="required">
			<xs:annotation>
				<xs:documentation>Ограничения операций по счету.</xs:documentation>
				<xs:documentation>Restriction on the account.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccRstrDate" type="lt:DateType">
			<xs:annotation>
				<xs:documentation>Дата начала действия Ограничения операций по счету.</xs:documentation>
				<xs:documentation>Restriction start date.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DateIn" type="lt:DateType" use="required">
			<xs:annotation>
				<xs:documentation>Дата открытия счета.</xs:documentation>
				<xs:documentation>Account’s Open Date.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DateOut" type="lt:DateType">
			<xs:annotation>
				<xs:documentation>Дата исключения информации о счете участника.</xs:documentation>
				<xs:documentation>Account’s Close Date.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccountStatus" type="lt:AccountStatusType">
			<xs:annotation>
				<xs:documentation>Статус счета.</xs:documentation>
				<xs:documentation>Account Status.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AdditionalConditions">
		<xs:annotation>
			<xs:documentation>Дополнительные условия проведения платежей или осуществления расчетов.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="UrgentPayments" type="lt:OneDigitCodeType">
			<xs:annotation>
				<xs:documentation>Разрешение экстренных платежей.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ChangeUrgentPayments" type="lt:Indicator1Type">
			<xs:annotation>
				<xs:documentation>Признак изменения реквизита "Разрешение экстренных платежей". </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SuspendedMode" type="lt:OneDigitCodeType">
			<xs:annotation>
				<xs:documentation>Вид обработки внутридневной очереди при завершении расчетов.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ChangeSuspendedMode" type="lt:Indicator1Type">
			<xs:annotation>
				<xs:documentation>Признак изменения реквизита "Вид обработки внутридневной очереди при завершении расчетов".</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SWIFTMode" type="lt:OneDigitCodeType">
			<xs:annotation>
				<xs:documentation>Признак взаимодействия через систему S.W.I.F.T.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ChangeSWIFTMode" type="lt:Indicator1Type">
			<xs:annotation>
				<xs:documentation>Признак изменения реквизита "Признак взаимодействия через систему S.W.I.F.T".</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LimitedMode" type="lt:OneDigitCodeType">
			<xs:annotation>
				<xs:documentation>Вид лимитирования платежей.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ChangeLimitedMode" type="lt:Indicator1Type">
			<xs:annotation>
				<xs:documentation>Признак изменения реквизита "Вид лимитирования платежей".</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LiquidityBfOD" type="lt:OneDigitCodeType">
			<xs:annotation>
				<xs:documentation>Разрешение установления Банком России ликвидности для расчетов ПУР до начала операционного дня.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ChangeLiquidityBfOD" type="lt:Indicator1Type">
			<xs:annotation>
				<xs:documentation>Признак изменения реквизита "Разрешение установления Банком России ликвидности для расчетов ПУР до начала операционного дня".</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LiquidityInOD" type="lt:OneDigitCodeType">
			<xs:annotation>
				<xs:documentation>Разрешение установления Банком России ликвидности для расчетов ПУР в течение операционного дня.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ChangeLiquidityInOD" type="lt:Indicator1Type">
			<xs:annotation>
				<xs:documentation>Признак изменения реквизита "Разрешение установления Банком России ликвидности для расчетов ПУР в течение операционного дня".</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ArrestDetailedInfo">
		<xs:annotation>
			<xs:documentation>Список арестов и/или ограничений.</xs:documentation>
			<xs:documentation>List of Arrests/Limitations.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RestrictionBasisDoc" type="ed:RestrictionBasisDocument" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Основание для ограничения участия в расчетах или исключения из состава участников расчетов (основание для наложения ареста/ограничения).</xs:documentation>
					<xs:documentation>Restriction or restricton’s deletion basis document.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ArrestID" type="lt:Max10TextType" use="required">
			<xs:annotation>
				<xs:documentation>Идентификатор ареста/ограничения.</xs:documentation>
				<xs:documentation>ID of Arrest/Limitation.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="IndeterminateAmountFlag" type="lt:IndicatorType" use="required">
			<xs:annotation>
				<xs:documentation>Индикатор ареста на неопределенную сумму.</xs:documentation>
				<xs:documentation>Indeterminate account Flag.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ArrestAmount" type="lt:NotNegativeKopeckAmountType">
			<xs:annotation>
				<xs:documentation>Сумма ареста/ограничения.</xs:documentation>
				<xs:documentation>Amount of arrest (limitation) imposed on the account.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OutstandingAmount" type="lt:NotNegativeKopeckAmountType">
			<xs:annotation>
				<xs:documentation>Неоплаченная сумма ареста/ограничения по счету.</xs:documentation>
				<xs:documentation>Outstanding amount of arrest or limitation on the account.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ArrestInfo">
		<xs:annotation>
			<xs:documentation>Общая сумма и общая неоплаченная сумма арестов и/или ограничений по счету.</xs:documentation>
			<xs:documentation>Total Amount of Arrest (Limitation) and Total Unpaid Amount of Arrest (Limitation) on account.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ArrestDetailedInfo" type="ed:ArrestDetailedInfo" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Список арестов и/или ограничений.</xs:documentation>
					<xs:documentation>List of Arrests/Limitations.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="TotalAmount" type="lt:NotNegativeKopeckAmountType">
			<xs:annotation>
				<xs:documentation>Общая сумма арестов и/или ограничений по счету.</xs:documentation>
				<xs:documentation>Total Amount of Arrest (Limitation) on account.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ArrestedAmount" type="lt:NotNegativeKopeckAmountType">
			<xs:annotation>
				<xs:documentation>Общая сумма средств, зарезервированная в счет арестов и/или ограничений по счету.</xs:documentation>
				<xs:documentation>Total Amount of funds reserved in consideration for Arrest (Limitation) to account.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OutstandingAmount" type="lt:NotNegativeKopeckAmountType">
			<xs:annotation>
